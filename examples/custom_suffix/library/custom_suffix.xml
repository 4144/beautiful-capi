<?xml version="1.0" encoding="utf-8" ?>
<custom_suffix:api xmlns:custom_suffix="http://gkmsoft.ru/beautifulcapi" project_name="custom_suffix">
  <namespace name="hello_world">
    <class name="printer" lifecycle="copy_semantic" implementation_class_name="hello_world::printer_impl" implementation_class_header="printer_impl.h">
      <constructor name="default"/>
      <method name="show" const="true"/>
    </class>
    <class name="scanner" lifecycle="raw_pointer_semantic" implementation_class_name="hello_world::scanner_impl" implementation_class_header="scanner_impl.h">
      <constructor name="default"/>
      <method name="scan" const="true"/>
    </class>
    <class name="plotter" lifecycle="reference_counted" implementation_class_name="hello_world::plotter_impl" implementation_class_header="plotter_impl.h">
      <constructor name="default"/>
      <method name="draw" const="true"/>
    </class>
  </namespace>
</custom_suffix:api>
